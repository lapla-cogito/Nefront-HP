{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off",
        "no-unused-vars": "off",
        "react/prop-types": "off",
        "react/display-name": "off",
        "no-undef": "off",
        "no-inner-declarations": "off",
        "no-redeclare": "off",
        "no-case-declarations": "off"
    },
    "globals": {
        "window": true,
        "process": true
    }
}
